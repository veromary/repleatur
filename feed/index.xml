---
layout: nil
permalink: /feed/
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Repleatur</title>
 <link href="http://www.repleatur.net/feed/" rel="self"/>
 <link href="http://www.repleatur.net/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.repleatur.net/</id>
 <author>
   <name>Repleatur</name>
   <email>mail@repleatur.net</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.repleatur.net{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.repleatur.net{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>

